!!! 5

%html
  %head
    %title <%= app_name %>
    = stylesheet_link_tag 'reset','admin'
    = javascript_include_tag :defaults
    = csrf_meta_tag

  %body{:id => "dashboard"}
    #container
      #header
        <%= app_name %>
        .user_nav
      #navigation
        = render :partial => "layouts/admin_nav"
      #content
        / =render :partial => "layouts/flashes"
        = yield
      #footer
        / =render :partial => "layouts/footer"
    / =render :partial => "layouts/tracking"